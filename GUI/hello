#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x91a3d20 .scope module, "main" "main" 2 1;
 .timescale 0 0;
S_0x91a3da0 .scope module, "top" "top" 2 8;
 .timescale 0 0;
    .scope S_0x91a3d20;
T_0 ;
    %vpi_call 2 3 "$display", "main";
    %vpi_call 2 4 "$finish";
    %end;
    .thread T_0;
    .scope S_0x91a3da0;
T_1 ;
    %vpi_call 2 10 "$display", "top";
    %vpi_call 2 11 "$finish";
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "hello.v";
